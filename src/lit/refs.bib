	file = {[Fre13]Air Data system fault modeling and detection.pdf:D\:\\Ressourcen\\Bibliothek\\Fachbücher\\MA_lit\\[Fre13]Air Data system fault modeling and detection.pdf:application/pdf},
}

@inproceedings{brzezinski_identification_2011,
	address = {Orlando, FL, USA},
	title = {Identification of sensor-only {MIMO} pseudo transfer functions},
	isbn = {978-1-61284-801-3 978-1-61284-800-6 978-1-4673-0457-3 978-1-61284-799-3},
	url = {http://ieeexplore.ieee.org/document/6160978/},
	doi = {10.1109/CDC.2011.6160978},
	abstract = {In many applications of system identiﬁcation, output measurements are available, but measurements of the input are not available. In these cases, sensor-only identiﬁcation techniques are needed. In the present paper, we deﬁne a discrete-time MIMO pseudo transfer function (PTF) between sensor measurements in a sampled-data system with multiple excitation signals. We show that the MIMO PTF does not depend on the unknown initial state or the unknown excitation. We also characterize the order and relative degree of the MIMO PTF. To consistently estimate the Markov parameters of the MIMO PTF in the presence of sensor noise, we use quadratically constrained least squares identiﬁcation for MIMO systems. We apply this technique to a three-degree-of-freedom mass-spring-damper system to assess the accuracy of the identiﬁed PTFs.},
	language = {en},
	urldate = {2023-03-31},
	booktitle = {{IEEE} {Conference} on {Decision} and {Control} and {European} {Control} {Conference}},
	publisher = {IEEE},
	author = {Brzezinski, Adam J. and Kukreja, Sunil L. and Ni, Jun and Bernstein, Dennis S.},
	month = dec,
	year = {2011},
	pages = {2154--2159},
	file = {[Brz11]Identification of Sensor-Only MIMO Pseudo Transfer Functions.pdf:D\:\\Ressourcen\\Bibliothek\\Fachbücher\\MA_lit\\[Brz11]Identification of Sensor-Only MIMO Pseudo Transfer Functions.pdf:application/pdf},
}

