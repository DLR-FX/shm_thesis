	month = dec,
	year = {2011},
	pages = {2154--2159},
	file = {[Brz11]Identification of Sensor-Only MIMO Pseudo Transfer Functions.pdf:D\:\\Ressourcen\\Bibliothek\\Fachbücher\\MA_lit\\[Brz11]Identification of Sensor-Only MIMO Pseudo Transfer Functions.pdf:application/pdf;Brzezinski et al. - 2011 - Identification of sensor-only MIMO pseudo transfer.pdf:C\:\\Users\\klei_cl\\Zotero\\storage\\ZZDW3DVV\\Brzezinski et al. - 2011 - Identification of sensor-only MIMO pseudo transfer.pdf:application/pdf},
