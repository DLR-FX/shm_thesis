@article{kornecki_approaches_nodate,
	title = {{APPROACHES} {TO} {ASSURE} {SAFETY} {IN} {FLY}-{BY}-{WIRE} {SYSTEMS}: {AIRBUS} {VS}. {BOEING}},
	abstract = {Fly-by-wire (FBW) is a flight control system using computers and relatively light electrical wires to replace conventional direct mechanical linkage between a pilot’s cockpit controls and moving surfaces. FBW systems have been in use in guided missiles and subsequently in military aircraft. The delay in commercial aircraft implementation was due to the time required to develop appropriate failure survival technologies providing an adequate level of safety, reliability and availability. Software generation contributes significantly to the total engineering development cost of the high integrity digital FBW systems. Issues related to software and redundancy techniques are discussed. The leading commercial aircraft manufacturers, such as Airbus and Boeing, exploit FBW controls in their civil airliners. The paper presents their approach, the difference of control philosophy, and the implementation resulting in a comparable level of safety assurance essential for airline operations.},
	language = {en},
	author = {Kornecki, Andrew J and Hall, Kimberley},
	file = {[Kor04]Approaches to assure safety in fly-by-wire systems airbus vs boeing.pdf:D\:\\Ressourcen\\Bibliothek\\Fachbücher\\MA_lit\\[Kor04]Approaches to assure safety in fly-by-wire systems airbus vs boeing.pdf:application/pdf;Kornecki und Hall - APPROACHES TO ASSURE SAFETY IN FLY-BY-WIRE SYSTEMS.pdf:C\:\\Users\\klei_cl\\Zotero\\storage\\V79HRATP\\Kornecki und Hall - APPROACHES TO ASSURE SAFETY IN FLY-BY-WIRE SYSTEMS.pdf:application/pdf},
