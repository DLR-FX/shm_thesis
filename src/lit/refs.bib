@article{khalil_transmissibility-based_2022,
	title = {Transmissibility-{Based} {Health} {Monitoring} of the {Future} {Connected} {Autonomous} {Vehicles} {Networks}},
	volume = {71},
	issn = {0018-9545, 1939-9359},
